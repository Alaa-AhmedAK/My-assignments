class Car:
    def __init__(self, brand, model, year):
        self.brand = brand
        self.model = model
        self.year = year
    
    def start_engine(self):
        return f"The {self.year} {self.brand} {self.model}'s engine is starting!"

my_car = Car("Toyota", "Corolla", 2023)
print(my_car.start_engine())
